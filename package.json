{
  "name": "murfey-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.3.6",
    "@chakra-ui/system": "^2.6.2",
    "@chakra-ui/toast": "^7.0.2",
    "@diamondlightsource/ui-components": "^2.2.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@tanstack/react-query": "^5.81.2",
    "framer-motion": "^11.11.4",
    "react": "^18.3.1",
    "react-collapsible": "^2.10.0",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1",
    "react-spinners": "^0.16.1",
    "react-use-websocket": "^4.5.0",
    "uuid": "^10.0.0",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "yarn prettier \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml,html,css,scss}\" --write",
    "lint": "yarn eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,html,css,scss}": "prettier --write"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@tanstack/react-query-devtools": "^5.81.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.5.0",
    "husky": "^8.0.0",
    "husky-init": "^8.0.0",
    "lint-staged": "^16.1.2",
    "openapi-typescript": "^6.7.2",
    "prettier": "^3.6.0",
    "react-scripts": "^5.0.1",
    "typescript": "^5"
  }
}
